{
  "name": "ai-model-gallery",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0 --port 3000",
    "dev:backend": "next dev --hostname 0.0.0.0 --port 3001",
    "dev:local": "next dev --port 3000",
    "build": "npx prisma generate && cross-env CI=false next build",
    "start": "npx prisma db push --accept-data-loss && next start",
    "start:production": "node scripts/startup-check.js && npm start",
    "start:backend": "next start --hostname 0.0.0.0 --port 3001",
    "start:local": "next start --port 3000",
    "startup-check": "node scripts/startup-check.js",
    "lint": "eslint",
    "security-check": "node scripts/security-check.js",
    "pre-deploy": "npm run security-check && npm run lint && npm run build",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "validate-env": "node scripts/validate-environment-simple.js",
    "health-check": "node scripts/health-check.js",
    "deploy-safe": "npm run validate-env && npm run pre-deploy && echo '✅ 배포 준비 완료 - Railway로 배포하세요'",
    "deploy-railway": "node scripts/railway-deploy.js",
    "post-deploy": "sleep 30 && npm run health-check",
    "batch-upload": "node scripts/batch-upload.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@playwright/test": "^1.55.1",
    "@prisma/client": "^6.15.0",
    "@tanstack/react-query": "^5.85.9",
    "bcryptjs": "^2.4.3",
    "critters": "^0.0.23",
    "form-data": "^4.0.4",
    "framer-motion": "^12.23.12",
    "masonic": "^4.1.0",
    "next": "15.5.2",
    "next-auth": "^4.24.11",
    "node-fetch": "^2.7.0",
    "playwright": "^1.55.1",
    "prisma": "^6.15.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-responsive-masonry": "^2.7.1",
    "sharp": "^0.34.3",
    "zustand": "^5.0.8"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
